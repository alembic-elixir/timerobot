<h2>Report Index</h2>


<table class="table table-striped">
  <thead>
    <tr>
      <th>Client</th>
      <th>Project</th>
    </tr>
  </thead>
  <tbody>
<%= for project <- @projects do %>
    <tr>
      <td><%= project.name %></td>
      <td><%= link(project.name, to: project_path(@conn, :show, project.id)) %></td>

      <td class="text-right">
      </td>
    </tr>
<% end %>
  </tbody>
</table>
